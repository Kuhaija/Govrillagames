<source src="/elämältäkaikensain/bensound-sexy.mp3" />
</audio>
<a href="#noscroll" id="togglebgm">toggle sound</a>

<script>
var audio = document.getElementById('audio_bgm');
             
document.getElementById('togglebgm').addEventListener('click', function (e)
{
e = e || window.event;
audio.muted = !audio.muted;
e.preventDefault();
}, false);
</script>


